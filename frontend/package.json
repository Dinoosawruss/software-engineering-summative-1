{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.[t|j]sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "parser": {
              "syntax": "ecmascript",
              "jsx": true
            },
            "transform": {
              "react": {
                "pragma": "React.createElement",
                "pragmaFrag": "React.Fragment"
              }
            }
          }
        }
      ]
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  },
  "dependencies": {
    "axios": "^1.7.9",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@swc/core": "^1.10.9",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "jest": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
